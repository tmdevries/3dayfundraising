<p id="notice"><%= notice %></p>

<h1>Listing Autobids</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @autobids.each do |autobid| %>
      <tr>
        <td><%= link_to 'Show', autobid %></td>
        <td><%= link_to 'Edit', edit_autobid_path(autobid) %></td>
        <td><%= link_to 'Destroy', autobid, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Autobid', new_autobid_path %>
